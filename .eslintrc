{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parser": "babel-eslint",
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-console": 0,
    "prettier/prettier": [
      "error",
      {
        "usePrettierrc": true
      }
    ]
  }
}